<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.finalfour.LeaderBoard">
   <children>
      <Label layoutX="250.0" layoutY="31.0" text="High Scores:" textAlignment="CENTER">
         <font>
            <Font name="Impact" size="20.0" />
         </font></Label>
      <Label fx:id="leaderBoard" layoutX="150.0" layoutY="78.0" text="1.SamplePerson - 25 points, 5 misses, tricked 4 times" />
      <TextField fx:id="nameInput" layoutX="175.0" layoutY="302.0" promptText="Enter Your Name Here" />
      <Button fx:id="nameSubmit" layoutX="330.0" layoutY="302.0" mnemonicParsing="false" onAction="#onSubmit" text="Submit" />
      <Button fx:id="parseButton" layoutX="264.0" layoutY="349.0" mnemonicParsing="false" onAction="#onClickParseHTML" text="Parse HTML" textAlignment="CENTER" />
      <Label fx:id="userResults" layoutX="220.0" layoutY="256.0" text="10 points, 8 misses, tricked 7 times" />
      <Button fx:id="clearButton" layoutX="14.0" layoutY="362.0" mnemonicParsing="false" onAction="#onClearButton" text="Don't click me" />
   </children>
</AnchorPane>
